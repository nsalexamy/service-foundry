{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "MlModelStorageAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::nsa2-sf-ml-models",
                "arn:aws:s3:::nsa2-sf-ml-models/*"
            ]
        }
    ]
}
